{
  "alias": "nemesis.alchematrix.net",
  "name": "PoCake-Nemesis",
  "env": {
    "STRIPE_PRIV_KEY": "@stripe_test_key",
    "AIRTABLE_KEY": "@airtable_key",
    "REACT_APP_STRIPE_PUB_KEY": "@react_app_stripe_pub_key",
    "REACT_APP_DEMO": "on",
    "TARGET": "now",
    "MONGODB_URI_PW": "@mongodb_uri_pw",
    "VAPID_PUBLIC_KEY": "key",
    "VAPID_PRIVATE_KEY": "@vapid_private_key",
    "VAPID_PUBLIC_KEY_PRESTA": "@vapid_public_key_presta",
    "VAPID_PRIVATE_KEY_PRESTA": "@vapid_private_key_presta",
    "SQUARE_PRIV_KEY": "@square_priv_key",
    "SQUARE_APP_ID": "@square_app_id",
    "SQUARE_LOCATION_ID": "@square_location_id"
  },
  "public": false,
  "version": 2,
  "routes": [
    {"src": "^/static/(.*)", "dest": "/static/$1"},
    {"src": "^/build/(.*)", "dest": "/build/$1"},
    {"src": "^/icons/(.*)", "dest": "/icons/$1"},
    {"src": "^/favicon.ico", "dest": "/favicon.ico"},
    {"src": "^/icecream.ico", "dest": "/icecream.ico"},
    {"src": "^/asset-manifest.json", "dest": "/asset-manifest.json"},
    {"src": "^/manifest.json", "dest": "/manifest.json"},
    {"src": "^/register-push-sw.js", "headers": {"cache-control": "s-maxage=0", "Content-Type": "text/javascript"}, "dest": "/register-push-sw.js"},
    {"src": "^/custom-sw.js", "headers": {"cache-control": "s-maxage=0", "Content-Type": "text/javascript"}, "dest": "/custom-sw.js"},
    {"src": "^/service-worker.js", "headers": {"cache-control": "s-maxage=0", "Content-Type": "text/javascript"}, "dest": "/service-worker.js"},
    {"src": "^/precache-manifest.(.*)", "dest": "/precache-manifest.$1"},
    {"src": "/charge", "dest": "server.js"},
    {"src": "/receivePushSubscription", "dest": "server.js"},
    {"src": "/markOrderReady", "dest": "server.js"},
    {"src": "/chargeSquareNonce", "dest": "server.js"},
    {"src": "/handleOrderReady", "dest": "server.js"},
    {"src": "/dispatchPush", "dest": "server.js"},
    {"src": "/testPush", "dest": "server.js"},
    {"src": "/serverUp", "dest": "server.js"},
    {"src": "/receivePush", "dest": "server.js"},
    { "src": "^/(.*)", "headers": {"cache-control": "s-maxage=0"}, "dest": "/index.html" }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    }, {
      "src": "server.js",
      "use": "@now/node",
      "config" : {
        "maxLambdaSize": "20mb"
      }
    }
  ]
}
